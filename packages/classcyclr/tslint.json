{
    "extends": "./node_modules/shenanigans-manager/setup/tslint.json",
    "linterOptions": {
        "exclude": [
            "./node_modules/**/*"
        ]
    },
    "rules": {
        "no-any": false,
        "no-dynamic-delete": false,
        "no-unsafe-any": false,
        "prefer-function-over-method": false,
        "strict-boolean-expressions": false,
        "strict-type-predicates": false
    }
}
